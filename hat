local ReGui = loadstring(game:HttpGet('https://raw.githubusercontent.com/depthso/Dear-ReGui/refs/heads/main/ReGui.lua'))()
local TextChatService = game:GetService("TextChatService")
local Players = game:GetService("Players")

function SendMsg(str,hide)
    if hide then
        TextChatService.TextChannels.RBXSystem:SendAsync(str)
    else
        TextChatService.TextChannels.RBXGeneral:SendAsync(str)
    end
end

local Window = ReGui:Window({
	Title = "APR | All Purpose Rabbitüêá",
	Size = UDim2.fromOffset(300, 200)
})

Window:Label({Text="28/09/2025"})
Window:Button({
	Text = "Join Large Server",
	Callback = function()
		SendMsg("joinxl",true)
	end,
})

local ModalWindow = ReGui:PopupModal({
	Title = "Delete?"
})

ModalWindow:Label({
	Text = "All those beautiful files will be deleted.\nThis operation cannot be undone!",
	TextWrapped = true
})
ModalWindow:Separator()
ModalWindow:Checkbox({
	Value = false,
	Label = "Don't ask me next time"
})

local Row = ModalWindow:Row({
	Expanded = true
})
Row:Button({
	Text = "Okay",
	Callback = function()
		ModalWindow:ClosePopup()
	end,
})
Row:Button({
	Text = "Cancel",
	Callback = function()
		ModalWindow:ClosePopup()
	end,
})
