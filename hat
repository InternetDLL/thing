local ReGui = loadstring(game:HttpGet('https://raw.githubusercontent.com/depthso/Dear-ReGui/refs/heads/main/ReGui.lua'))()
local TextChatService = game:GetService("TextChatService")
local Players = game:GetService("Players")

function SendMsg(str,hide)
    if hide then
        TextChatService.TextChannels.RBXSystem:SendAsync(str)
    else
        TextChatService.TextChannels.RBXGeneral:SendAsync(str)
    end
end

local Window = ReGui:Window({
	Title = "all purpose rabbit🐇",
	Size = UDim2.fromOffset(300, 200)
}) --> Canvas & WindowClass

Window:Label({Text="28/09/2025"})
Window:Button({
	Text = "Join Large Server",
	Callback = function()
		SendMsg("joinxl",true)
	end,
})
Window:InputText({Label="string"})
Window:SliderFloat({Label = "float", Minimum = 0.0, Maximum = 1.0})
